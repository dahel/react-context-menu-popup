{
  "name": "react-context-menu-popup",
  "version": "1.0.10",
  "description": "Simple, mobile friendly context menu popup for react-based web apps",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "./node_modules/.bin/babel src -d lib",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js --recursive \"./test/**/*@(.test.js|.test.jsx)\"",
    "test-watch": "mocha --watch --compilers js:babel-core/register --require ./test/setup.js --recursive \"./test/**/*@(.test.js|.test.jsx)\"",
    "cover": "cd ./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec \"./test/**/*@(.test.js|.test.jsx)\"",
    "dev-cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require ./test/setup.js --recursive \"./test/**/*@(.test.js|.test.jsx)\"",
    "te": "./node_modules/.bin/mocha --compilers js:babel-core/register --require ./test/setup.js blanket --reporter html \"./test/**/*@(.test.js|.test.jsx)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dahel/react-context-menu-popup.git"
  },
  "author": {
    "name": "Sebastian Prymas",
    "email": "prymas.sebastian@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dahel/react-context-menu-popup/issues"
  },
  "homepage": "https://github.com/dahel/react-context-menu-popup#readme",
  "peerDependencies": {
    "react": ">=15.1.0",
    "react-dom": ">=15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "6.5.1",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.0",
    "glob": "^7.1.1",
    "istanbul": "^0.4.4",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.1.0",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0"
  }
}
